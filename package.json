{
  "name": "one-kyobashi-commons-server",
  "version": "0.1.0",
  "description": "Server for ONE Kyobashi Commons app",
  "license": "MIT",
  "author": "tsato",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "npm run build && concurrently \"npx tsc -w\" \"nodemon --exec ts-node src/index.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:generate": "npx drizzle-kit generate --config=drizzle-dev.config.ts",
    "db:migrate": "tsx ./src/db/migrate.ts",
    "db:seed": "tsx ./src/db/seed.ts"
  },
  "dependencies": {
    "@types/multer": "^1.4.12",
    "@types/pg": "^8.15.5",
    "aws-sdk": "^2.1692.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.5",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "pg": "^8.16.3",
    "pluralize": "^8.0.0",
    "serverless-http": "^3.2.0",
    "stripe": "^18.2.1",
    "tsx": "^4.20.5",
    "uuid": "^11.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.18.0",
    "@types/pluralize": "^0.0.33",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.31.4",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
